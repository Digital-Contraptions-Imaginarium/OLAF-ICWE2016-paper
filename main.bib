author = {Andr{\'e}, Paul and Zhang, Haoqi and Kim, Juho and Chilton, Lydia B and Dow, Steven P and Miller, Robert C},
title = {{Community Clustering: Leveraging an Academic Crowd to Form Coherent Conference Sessions}},
journal = {First AAAI Conference on Human Computation and Crowdsourcing},
year = {2013},
month = nov,
language = {English},
rating = {0},
date-added = {2015-08-27T06:43:56GMT},
date-modified = {2015-10-07T20:46:44GMT},
abstract = {Creating sessions of related papers for a large conference is a complex and time-consuming task. Traditionally, a few conference organizers group papers into sessions manually. Organizers often fail to capture the affinities between papers beyond created sessions, making incoherent sessions difficult to fix and alternative groupings hard to discover. This paper proposes committeesourcing and authorsourcing approaches to session creation (a specific instance of clustering and constraint satisfaction) that tap into the expertise and interest of committee members and authors for identifying paper affinities. During the planning of ACM CHI'13, a large conference on human-computer interaction, we recruited committee members to group papers using two online distributed clustering methods. To refine these paper affinities {\textemdash} and to evaluate the committeesourcing methods against existing manual and automated approaches {\textemdash} we recruited authors to identify papers that fit well in a session with their own. Results show that authors found papers grouped by the distributed clustering methods to be as relevant as, or more relevant than, papers suggested through the existing in-person meeting. Results also demonstrate that communitysourced results capture affinities beyond sessions and provide flexibility during scheduling.},
url = {http://www.aaai.org/ocs/index.php/HCOMP/HCOMP13/paper/view/7512},
local-url = {file://localhost/Users/giacecco/Dropbox/Apps/Papers/Library.papers3/Files/DE/DEAADB87-89F9-464B-A83B-D7B852162989.pdf},
