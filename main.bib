@article{Giannotti:2012da,
author = {Giannotti, F and Pedreschi, D and Pentland, Alex Sandy and Lukowicz, P and Kossmann, Donald and Crowley, J and Helbing, D},
title = {{A planetary nervous system for social mining and collective awareness}},
journal = {European Physical Journal-Special Topics},
year = {2012},
volume = {214},
number = {1},
pages = {49--75},
month = nov,
annote = {- Focus is on using the "PNS" to understand society more than as a tool for data mining. After all, though, our problem is a subset and easier problem than the one the authors are trying to address.},
publisher = {Springer-Verlag},
keywords = {crowd mining, participatory sensing, social mining, social sensing},
doi = {10.1140/epjst/e2012-01688-9},
language = {English},
read = {Yes},
rating = {0},
date-added = {2015-06-15T09:59:34GMT},
date-modified = {2015-07-24T07:56:06GMT},
abstract = {We present a research roadmap of a Planetary Nervous System (PNS), capable of sensing and mining the digital breadcrumbs of human activities and unveiling the knowledge hidden in the big data for addressing the big questions about social complexity. We envision the PNS as a globally distributed, self-organizing, techno-social system for answering analytical questions about the status of world-wide soci- ety, based on three pillars: social sensing, social mining and the idea of trust networks and privacy-aware social mining. We discuss the ingre- dients of a science and a technology necessary to build the PNS upon the three mentioned pillars, beyond the limitations of their respective state-of-art. Social sensing is aimed at developing better methods for harvesting the big data from the techno-social ecosystem and make them available for mining, learning and analysis at a properly high ab- straction level. Social mining is the problem of discovering patterns and models of human behaviour from the sensed data across the various so- cial dimensions by data mining, machine learning and social network analysis. Trusted networks and privacy-aware social mining is aimed at creating a new deal around the questions of privacy and data own- ership empowering individual persons with full awareness and control on own personal data, so that users may allow access and use of their data for their own good and the common good. The PNS will provide a goal-oriented knowledge discovery framework, made of technology and people, able to configure itself to the aim of answering questions about the pulse of global society. Given an analytical request, the PNS activates a process composed by a variety of interconnected tasks ex- ploiting the social sensing and mining methods within the transparent ecosystem provided by the trusted network. The PNS we foresee is the key tool for individual and collective awareness for the knowledge so- ciety. We need such a tool for everyone to become fully aware of how powerful is the knowledge of our society we can achieve by leveraging our wisdom as a crowd, and how important is that everybody partic- ipates both as a consumer and as a producer of the social knowledge, for it to become a trustable, accessible, safe and useful public good.},
url = {http://www.springerlink.com/index/10.1140/epjst/e2012-01688-9},
local-url = {file://localhost/Users/giacecco/Dropbox/Apps/Papers/Library.papers3/Files/99/99C905D5-81CB-4A1B-9D8A-38D14F4D1DFC.pdf},
file = {{99C905D5-81CB-4A1B-9D8A-38D14F4D1DFC.pdf:/Users/giacecco/Dropbox/Apps/Papers/Library.papers3/Files/99/99C905D5-81CB-4A1B-9D8A-38D14F4D1DFC.pdf:application/pdf;99C905D5-81CB-4A1B-9D8A-38D14F4D1DFC.pdf:/Users/giacecco/Dropbox/Apps/Papers/Library.papers3/Files/99/99C905D5-81CB-4A1B-9D8A-38D14F4D1DFC.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1140/epjst/e2012-01688-9}}
}

